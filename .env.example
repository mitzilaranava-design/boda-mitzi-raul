# Supabase - copiar a .env y reemplazar con tus valores
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_ANON_KEY=tu-anon-key

# Token de acceso para la página principal (Save the Date)
# Incluirlo en el link de WhatsApp: https://sitio.com/?t=TU_TOKEN
VITE_ACCESS_TOKEN=

# Panel de administración
# Acceso: https://sitio.com/admin?t=TU_ADMIN_TOKEN
VITE_ADMIN_TOKEN=
# URL del sitio (sin / al final)
VITE_SITE_URL=https://tu-sitio.com
# Intervalo de recordatorios en minutos (10080 = 1 semana, 1 = testing)
VITE_REMINDER_INTERVAL_MINUTES=10080
